<resources xmlns:android="http://schemas.android.com/apk/res/android"><!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.

    --><style name="AppBaseTheme" parent="android:Theme.Light"><!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.

        --></style><!-- Application theme. --><style name="AppTheme" parent="AppBaseTheme"><!-- All customizations that are NOT specific to a particular API-level can go here. --><item name="android:windowNoTitle">true</item></style><style name="LoadingTheme" parent="AppBaseTheme"><item name="android:windowBackground">@drawable/loading_bg</item><item name="android:windowNoTitle">true</item></style><style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox"><item name="android:textSize">16.0sp</item><item name="android:textColor">@drawable/qq_checkbox_text_color</item><item name="android:paddingLeft">28.0px</item><item name="android:button">@drawable/qq_btn_check</item></style><!--定义对话框样式--><style name="dialog" parent="@android:style/Theme.Dialog"><!-- 边框 --><item name="android:windowFrame">@null</item><!-- 是否浮现在activity之上 --><item name="android:windowIsFloating">true</item><!-- 半透明 --><item name="android:windowIsTranslucent">true</item><!-- 无标题 --><item name="android:windowNoTitle">true</item><!-- 背景透明 --><item name="android:windowBackground">@color/touming</item><!-- 模糊 --><item name="android:backgroundDimEnabled">true</item></style></resources>